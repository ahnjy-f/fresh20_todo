<%-include('./header') %>

<h1 class="mt-3 mb-3"><%= __n('Admin2.Admin2') %></h1>

<script>
    function del(id){
        var result = window.confirm('id=' + id + 'のユーザーを削除しますか');
        if(result){
            window.location = '/users/del_user?id=' + id;
        }
    }
</script>
<p class="h5 mt-4"><%= __n('Admin2.Registered users') %></p>
<ul class="list-group">
    <% for(let i in data){ %>
        <li class="list-group-item list-group-item-action">
            <%= data[i].id + ': ' + data[i].account %>
            <%if(data[i].role!="admin"){%>
            <strong>
                <a href="javascript:del(<%=data[i].id %>)">[<%= __n('Admin2.delete') %>]</a>
            </strong>
       <% }%>
        </li>
    
    <%}%>
</ul>
</p>
<%-include('./footer')%>
