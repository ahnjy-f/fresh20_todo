<%-include('./header') %>

<h1 class="mt-3 mb-3"><%= __n('Admin.Admin') %></h1>

<p class="h5 mt-4"><%= __n('Admin.Add new User') %></p>
<form method="POST" action="/users/admin">
    <div class="form-group">
        <label for="account"><%= __n('Admin.Account') %>t</label>
        <input type="mail" id ="account" name="account"
          class="form-control">
    </div>
    <div class="form-group">
        <label for="password"><%= __n('Admin.Password') %></label>
        <input type="text" id ="password" name="password"
          class="form-control">
    </div>
    <div class="form-group">
        <label for="name"><%= __n('Admin.Name') %></label>
        <input type="text" id ="name" name="name"
          class="form-control">
    </div>
    <div class="form-group">
        <input type="submit"   class="btn btn-primary">
    </div>
</form>

<%-include('./footer')%>
